<style>
    
    .arrow_div {
        align-items: center;
        height: fit-content;
        display: flex;
        flex-direction: column;
        padding: 0;
        margin: 0;
        opacity: 0;
        animation: fadeIn 0.5s ease-out;
        animation-fill-mode: forwards;
        animation-delay: 4.5s;
        transform: translateY(5vh);
    }

    .arrow {
        scale: 1;
        width: 8vw;
        filter: brightness(0);
        height: auto;
    }

    #arrow1 {
        animation: lightUp 2s linear infinite;
        animation-delay: 5s;
        animation-fill-mode: forwards;
    }

    #arrow2 {
        animation: lightUp 2s linear infinite;
        animation-delay: 5.5s;
        animation-fill-mode: forwards;
    }

    #arrow3 {
        animation: lightUp 2s linear infinite;
        animation-delay: 6s;
        animation-fill-mode: forwards;
    }

    @keyframes fadeIn {
        0% {
            opacity: 0;
        }

        100% {
            opacity: 1;
        }
    }

    @keyframes lightUp {
        0% {
            filter: brightness(0.5);
        }

        25% {
            filter: brightness(1)
        }

        50% {
            filter: brightness(0.5);
        }

        100% {
            filter: brightness(0.5);
        }
    }
</style>