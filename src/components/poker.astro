---
export interface Props {
  src: string;
}

const { src } = Astro.props as Props;
const { default: innerHTML } = await import(`/src/svgs/${src}.svg?raw`);
---

<Fragment class="pokerCards" set:html={innerHTML} />

<style>
    .pokerCards {
        width: 12vw;
        border: 1px solid white;
    }
</style>